/**index.wxss**/
page {
  width: 100%;
  height: 100%;
  background: #f7f7f7;
}
#user {
  width: 100%;
  height: 100%;
}
/* 用户统计 */
.user-statistics {
  width: 100%;
  height: 275rpx;
  background: #ffffff;
  margin-top: 20rpx;
}
.user-statistics .title {
  height:100rpx;
  line-height: 100rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:800;
  color:rgba(51,51,51,1);
  margin-left: 15rpx;
}
.user-statistics .content {
  display: flex;
  justify-content: space-around;
  width: 100%;
  height: 175rpx;
  box-sizing: border-box;
  padding: 25rpx 0 49rpx;
}
.user-statistics .content > .classify + .classify {
  border-left: 1px solid #e7e7e7;
}
.user-statistics .content .classify  {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap:wrap;
}
.user-statistics .content .classify view {
  font-size:46rpx;
  font-family:DIN;
  font-weight:500;
  width: 100%;
  text-align:center;
}
.user-statistics .content .classify1 view {
  color:rgba(91,133,251,1);
}
.user-statistics .content .classify2 view {
  color:rgba(100,194,55,1);
}
.user-statistics .content .classify3 view {
  color:rgba(238,171,64,1);
}
.user-statistics .content .classify text {
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(153,153,153,1);
}

/* 用户画像-性别比例 */
.user-paint {
  width: 100%;
  height: 439rpx;
  margin-top: 20rpx;
  background: #ffffff;
  box-sizing:border-box;
  padding: 0 15rpx;
}
.user-paint .content {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  border-bottom: 1px dashed #d7d7d7;
}
.user-paint .title {
  height:100rpx;
  line-height: 100rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:800;
  color:rgba(51,51,51,1);
}
.user-paint .sex-percentange {
  height: 82rpx;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 60rpx;
  color: #5b85fb;
}
.user-paint .sex-percentange text {
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(102,102,102,1);
  margin: 0 6rpx;
}
.user-paint .percentange-chart {
  width: 100%;
  box-sizing: border-box;
  padding: 0 48rpx;
  margin-top: 55rpx;
}
.user-paint .percentange-chart .image {
  display: flex;
  justify-content: space-between;
  margin-bottom: 10rpx;
}
.user-paint .percentange-chart .image image {
  width: 42rpx;
  height: 42rpx;
}
.user-paint .percentange-chart .sex-classify {
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-top: 27rpx;
}
.user-paint .sex-classify view text {
  font-size:24rpx;
  font-weight:500;
  color:rgba(153,153,153,1);
}
.user-paint .sex-classify view .percentange {
  font-size:28rpx;
  font-weight:bold;
  margin-left: 6rpx;
}
.user-paint .sex-classify view .male {
  color:rgba(60,123,255,1);
}
.user-paint .sex-classify view .female {
  color:rgba(255,180,21,1);
}

/* 用户画像-年龄分布 */
.age-distribute {
  width: 100%;
  height: 586rpx;
  background: #ffffff;
  padding: 0 15rpx;
  box-sizing: border-box;
}
.age-distribute .content {
  width: 100%;
  height: 100%;
  box-sizing:border-box;
  border-bottom: 1px dashed #d7d7d7;
}
.age-distribute .age-percentange {
  height: 172rpx;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 60rpx;
  color: #5b85fb;
}
.age-distribute .age-percentange text {
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(102,102,102,1);
  margin: 0 6rpx;
}
.age-distribute .age-classify {
  width: 100%;
  display: grid;
  grid-template-columns: repeat(3, minmax(33%, 1fr));
  grid-gap: 10rpx;
}
.age-distribute .age-classify .circle {
  height: 200rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.age-distribute .age-classify .circle .age-region view {
  font-size:34rpx;
  font-weight:500;
  color:rgba(91,133,251,1);
}
.age-distribute .age-classify .circle .regiontwo view {
  color:rgba(133,203,18,1);
}
.age-distribute .age-classify .circle .regionthree view {
  color:rgba(3,199,186,1);
}
.age-distribute .age-classify .circle .regionfour view {
  color:rgba(255,175,18,1);
}
.age-distribute .age-classify .circle .regionfive view {
  color:rgba(255,94,42,1);
}
.age-distribute .age-classify .circle .regionsix view {
  color:rgba(168,66,238,1);
}
.age-distribute .age-classify .circle .age-region text {
  font-size:18rpx;
  font-weight:500;
  color:rgba(153,153,153,1);
}

/* 用户画像-职业分布-饼图 */
.profession-distribute {
  width: 100%;
  height: 564rpx;
  background: #ffffff;
}
.profession-distribute .profession-percentange {
  height: 172rpx;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 60rpx;
  color: #5b85fb;
}
.profession-distribute .profession-percentange text {
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(102,102,102,1);
  margin: 0 6rpx;
}
.profession-distribute .profession-chart {
  width: 100%;
  height: 392rpx;
}
.profession-distribute .profession-chart ec-canvas {
  width: 100%;
  height: 100%;
}

/* 用户分析-柱状图 */
.bar-chart {
  width: 100%;
  height: 554rpx;
  background: #ffffff;
  margin-top: 20rpx;
  box-sizing: border-box;
  padding: 0 15rpx;
}
.bar-chart .content {
  width: 100%;
  height: 100%;
}
.bar-chart .title {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
}
.bar-chart .title view{
  font-size:32rpx;
  height: 100%;
  line-height: 100rpx;
  font-weight:800;
  color:rgba(51,51,51,1);
}
.bar-chart .title text {
  font-size:28rpx;
  font-weight:500;
  display:block;
  line-height: 100rpx;
  color:rgba(51,51,51,1);
}
.bar-chart .user-analysis-chart {
  width: 100%;
  height: 454rpx;
}

/* 平均月活-折线图 */
.line-chart {
  width: 100%;
  height: 620rpx;
  background: #ffffff;
  margin-top: 20rpx;
  box-sizing: border-box;
  padding: 0 15rpx;
  margin-bottom: 54rpx;
}
.line-chart .content {
  width: 100%;
  height: 100%;
}
.line-chart .title {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
}
.line-chart .title view{
  font-size:32rpx;
  height: 100%;
  line-height: 100rpx;
  font-weight:800;
  color:rgba(51,51,51,1);
}
.line-chart .title text {
  font-size:28rpx;
  font-weight:500;
  display:block;
  line-height: 100rpx;
  color:rgba(51,51,51,1);
}
.line-chart .avange-analysis-chart {
  width: 100%;
  height: 529rpx;
}